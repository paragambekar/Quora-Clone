<div id="header">

    <div id="container">

        <div class="logo">
            <a href="/">
                <img src="/images/logo1.png">
            </a>
            
        </div>

        <div class="auth-links">
            <% if (locals.user){ %>
                
                <div class="links">
                    <a href="/users/profile/<%= user.id %>"><%= user.name %></a>
                </div>
                <div class="links">
                    <a href="/users/sign-out">Sign Out</a>
                </div>
    
            <% }else{ %>
    
                <div class="links">
                    <a href="/users/sign-in">Sign In</a>
                </div>
                <div class="links">
                    <a href="/users/sign-up">Sign Up</a>
                </div>
    
            <% } %>    
            
        </div>

    </div>

    

</div>

